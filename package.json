{
  "name": "tickr-workflow",
  "version": "1.0.0",
  "description": "Temporal worflow for Tickr",
  "main": "index.js",
  "engines": {
    "node": "17.x"
  },
  "scripts": {
    "build": "tsc -p . && npm run cp",
    "cp": "cpx \"./src/certs/*.*\" \"lib/certs\"",
    "build:tickr": "tsc --build ./tsconfig.json",
    "build:workflow": "ts-node ./src/scripts/build-workflow-bundle.ts",
    "lint": "eslint . --fix",
    "start": "node ./lib/worker.js && node ./lib/client.js",
    "start:worker": "node ./lib/worker.js",
    "start:client": "node ./lib/client.js",
    "start:cert": "ts-node src/cert.ts",
    "dev:activities": "ts-node ./src/activities/priceActionPosition.ts",
    "dev:start": "ts-node src/worker.ts && ts-node src/client.ts",
    "dev:worker": "ts-node src/worker.ts",
    "dev:client": "ts-node src/client.ts",
    "test": "jest --no-cache --maxWorkers=50%",
    "test:watch": "jest --watchAll --no-cache --maxWorkers=25%"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testTimeout": 300000,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quan005/Tickr-workflow.git"
  },
  "author": "DaQuan Freeman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quan005/Tickr-workflow/issues"
  },
  "homepage": "https://github.com/quan005/Tickr-workflow#readme",
  "keywords": [],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.15",
    "ts-node-dev": "^1.1.8"
  },
  "dependencies": {
    "@kafkajs/confluent-schema-registry": "^3.2.0",
    "@temporalio/activity": "^1.5.2",
    "@temporalio/client": "^1.5.2",
    "@temporalio/worker": "^1.5.2",
    "@temporalio/workflow": "^1.5.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/http-proxy": "^1.17.8",
    "@types/jest": "^29.2.5",
    "@types/kafkajs": "^1.9.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^18.11.13",
    "@types/tunnel": "^0.0.3",
    "@types/websocket": "^1.0.5",
    "@types/ws": "^8.5.4",
    "axios": "^0.25.0",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "cpx2": "^4.2.0",
    "date-holidays": "^3.16.17",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "http-proxy": "^1.18.1",
    "jest": "^29.3.1",
    "kafkajs": "^1.15.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "node-forge": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^13.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tunnel": "^0.0.6",
    "typescript": "^4.9.4",
    "ws": "^8.4.0"
  }
}
